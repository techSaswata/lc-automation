    <key>StandardErrorPath</key>
    <string>/Users/techsaswata/Downloads/lc-automation/local_runner_error.log</string>
    
    <key>StartInterval</key>
    <integer>3600</integer>
    
    <key>ThrottleInterval</key>
    <integer>600</integer>
</dict>
</plist>

